n = int(input('Digite um número:'))
tot = 0
for c in range (1, n+1):
    if n % c == 0:
        print('\33[34m', end=' ')
        tot += 1
    else:
        print('\33[31m', end=' ')
    print(f'{c}', end='')
print(f'\n \33[mO número {n} é divisivel {tot} vezes.')
if tot ==2:
    print('E por isso ele é primo')
else:
    print('E por isso ele não é primo')
